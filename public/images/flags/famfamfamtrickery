f = File.new 'file'
lines = f.readlines
lines = lines.select { |line| line.size == 7 }
lines = lines.map { |line| line[0..1] }

copy file http://www.iso.org/iso/iso3166_en_code_lists.txt
to a new file called iso.txt

f = File.new 'iso.txt'
f.readline # get rid of the notes
iso = f.readlines

hashed_iso = {}

iso.select { |a| !a.rstrip.empty? }.map do |b| 
  aux = b.rstrip.split ';' 
  hashed_iso[aux[1].downcase] = aux[0].capitalize
end

#need to take care of base64

iso_famfamfam = hashed_iso.select { |k,v| lines.member? k }

